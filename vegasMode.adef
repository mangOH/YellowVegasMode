executables:
{
    dhubPush = ( dhubPusher )
}

processes:
{
    run:
    {
        ( /bin/vegas )
    }
}

bundles:
{
    file:
    {
        [rx] vegas /bin/
    }
}

requires:
{
    file:
    {
        /bin/sh /bin/
        /usr/bin/expr /bin/
    }
}

bindings:
{
    dhubPush.dhubPusher.dhubAdmin -> dataHub.admin
}
